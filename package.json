{
  "name": "sodenoura",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "main.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "dev": "vite",
    "build": "vite build && npm run format-html",
    "production": "vite build",
    "preview": "vite preview",
    "format-html": "prettier --write dist/**/*.html",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint \"src/**/*.{js,mjs,cjs}\"",
    "lint:css": "stylelint \"src/**/*.{css,scss}\"",
    "lint:fix": "npm run lint:js:fix && npm run lint:css:fix",
    "lint:js:fix": "eslint \"src/**/*.{js,mjs,cjs}\" --fix",
    "lint:css:fix": "stylelint \"src/**/*.{css,scss}\" --fix",
    "format": "prettier --write \"src/**/*.{js,mjs,cjs,json,html,css,scss}\"",
    "format:check": "prettier --check \"src/**/*.{js,mjs,cjs,json,html,css,scss}\""
  },
  "devDependencies": {
    "autoprefixer": "^10.4.19",
    "directory-crawler": "^0.0.6",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-plugin-import": "^2.29.1",
    "postcss": "^8.4.38",
    "postcss-cli": "^11.0.0",
    "postcss-import": "^16.1.0",
    "prettier": "^3.2.5",
    "randomstring": "^1.3.0",
    "rimraf": "^5.0.5",
    "sass": "^1.72.0",
    "sharp": "^0.33.3",
    "stylelint": "^16.2.1",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-recess-order": "^5.0.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "svgo": "^3.2.0",
    "vite": "^5.2.7",
    "vite-plugin-handlebars": "^2.0.0",
    "vite-plugin-image-optimizer": "^1.1.7",
    "vite-plugin-minify": "^1.5.2"
  },
  "dependencies": {
    "@vercel/edge": "^1.2.2",
    "aos": "^2.3.4",
    "basic-auth": "^2.0.1",
    "flatpickr": "^4.6.13",
    "fs": "^0.0.1-security",
    "micromodal": "^0.4.10",
    "static-auth": "^2.1.2"
  }
}
